<main>
  {{#with car}}
  <h1>Edit: {{name}}</h1>
  <div class="form">
    <form action="/products/edit/{{_id}}" method="POST">
      <label for="name">Name</label>
      <input type="text" value="{{name}}" id="name" name="name" />
      <label for="name">Description</label>
      <textarea id="name" name="description">{{description}}</textarea>
      <label for="image">ImageUrl</label>
      <input type="text"value="{{imagUrl}}" id="image" name="imageUrl" />
      <label for="price">Price</label>
      <input type="text" id="price" value="{{price}}" name="price"></input>
      <input type="submit" value="Confirm" />
    </form>
  </div>
  {{/with}}
</main>